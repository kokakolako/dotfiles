hi -a Null      color: #7e40a5; font-weight: bold;
hi -a Number    color: #7e40a5;
hi -a Object    color: #3579a8;
hi -a String    color: #E6DB74;
hi -a Comment   color: #9999aa;
hi -a Key       font-weight:bold;
